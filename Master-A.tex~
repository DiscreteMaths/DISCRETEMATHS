  \begin{multicols}{2}
    \begin{itemize}
    \item[a)] $(110101)_{2}$ - $(1010111)_{2}$
    \item[b)] $(1010101)_{2}$ - $(101010)_{2}$
    \item[c)] $(11001010)_{2}$ - $(10110101)_{2}$
    \item[d)] $(1011001)_{2}$ - $(111010)_{2}$
    \end{itemize}
  \end{multicols}


