{\LARGE

\[
{\begin{pmatrix}
1&1 & 6  &8 & 5
\\  0&1&6 &0 & 6
\\  0&0&0 &1 & 7
\\  0&0&1 &1 & 7
\end{pmatrix}}
\]
}
\end{frame}


