\begin{frame}[fragile]
\frametitle{Reduced Row Echelon Form}

{\LARGE

\[
{\begin{pmatrix}
1&2 & 6  &3 & 5
\\  0&1&4 &0 & 6
\\  0&1&2 &1 & 7
\\  0&0&1 &1 & 7
\end{pmatrix}}
\]
}
\end{frame}
%-----------------------------------------------------------------------------%
\begin{frame}[fragile]
\frametitle{Reduced Row Echelon Form}

{\LARGE

\[
{\begin{pmatrix}
1&1 & 6  &8 & 5
\\  0&2&6 &0 & 6
\\  0&0&1 &1 & 7
\\  0&0&0 &1 & 7
\end{pmatrix}}
\]
}
\end{frame}
%-----------------------------------------------------------------------------%
\begin{frame}[fragile]
